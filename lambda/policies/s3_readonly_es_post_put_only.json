{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "S3",
        "Effect": "Allow",
        "Action": [
          "s3:Get*",
          "s3:List*",
          "s3:HeadBucket"
        ],
        "Resource": "*"
      },
      {
        "Sid": "ES",
        "Effect": "Allow",
        "Action": [
          "es:ESHttpPost",
          "es:ESHttpPut"
        ],
        "Resource": "*"
      }
    ]
  }
